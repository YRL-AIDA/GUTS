@prefix scn: <http://example.org/science#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ==================== Classes ====================

scn:MeasurementUnit a rdfs:Class ;
    rdfs:label "Unit of measurement"@en , "Единица измерения"@ru ;
    rdfs:comment "Unit conversion"@en , "Пересчет единиц измерения"@ru .

# ==================== Datatype Properties ====================

scn:category a rdf:Property ;
    rdfs:domain scn:MeasurementUnit ;
    rdfs:range xsd:decimal ;
    rdfs:label ""@en , ""@ru ;
    rdfs:comment ""@en , ""@ru .

scn:jetCentralityLow a rdf:Property ;
    rdfs:domain scn:MeasurementUnit ;
    rdfs:range xsd:decimal ;
    rdfs:label ""@en , ""@ru ;
    rdfs:comment ""@en , ""@ru .

scn:jetCentralityHigh a rdf:Property ;
    rdfs:domain scn:MeasurementUnit ;
    rdfs:range xsd:decimal ;
    rdfs:label ""@en , ""@ru ;
    rdfs:comment ""@en , ""@ru .

scn:crossSectionValue a rdf:Property ;
    rdfs:domain scn:MeasurementUnit ;
    rdfs:range xsd:decimal ;
    rdfs:label ""@en , ""@ru ;
    rdfs:comment ""@en , ""@ru .

scn:uncertaintyType a rdf:Property ;
    rdfs:domain scn:MeasurementUnit ;
    rdfs:range xsd:string ;
    rdfs:label ""@en , ""@ru ;
    rdfs:comment ""@en , ""@ru .
